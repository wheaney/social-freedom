{
  "name": "@social-freedom/infrastructure",
  "version": "0.0.7",
  "description": "Open-source social networking framework where each user account owns their infrastructure",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "postbuild": "cp ./package.json ./dist && cp LICENSE ./dist",
    "prepack": "npm run build",
    "federalstackcheck": "npm run build && cdk -v -a \"ts-node src/federal/app.ts\" list",
    "federalstackdeploy": "npm run build && cdk -v -a \"ts-node src/federal/app.ts\" deploy",
    "userstackcheck": "npm run build && cdk -v -a \"ts-node src/user/app.ts\" list",
    "userstackdeploy": "npm run build && cdk -v -a \"ts-node src/user/app.ts\" deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheaney/social-freedom.git"
  },
  "keywords": [
    "social-freedom"
  ],
  "author": "Wayne Heaney",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/wheaney/social-freedom/issues"
  },
  "homepage": "https://github.com/wheaney/social-freedom#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.19.0",
    "@aws-cdk/aws-cloudformation": "^1.19.0",
    "@aws-cdk/aws-cloudfront": "^1.19.0",
    "@aws-cdk/aws-cloudtrail": "^1.19.0",
    "@aws-cdk/aws-cloudwatch": "^1.19.0",
    "@aws-cdk/aws-dynamodb": "^1.19.0",
    "@aws-cdk/aws-ec2": "^1.19.0",
    "@aws-cdk/aws-events": "^1.19.0",
    "@aws-cdk/aws-lambda": "^1.19.0",
    "@aws-cdk/aws-route53": "^1.19.0",
    "@aws-cdk/aws-s3": "^1.19.0",
    "@aws-cdk/aws-s3-deployment": "^1.19.0",
    "@aws-cdk/aws-sns": "^1.19.0",
    "@aws-cdk/core": "^1.19.0",
    "@aws-cdk/cx-api": "^1.19.0",
    "@aws-cdk/region-info": "^1.19.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "aws-cdk": "^1.19.0",
    "jest": "^24.9.0",
    "lerna": "^3.20.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@social-freedom/federal-lambdas": "^0.0.7",
    "@social-freedom/user-lambdas": "^0.0.7",
    "@social-freedom/website": "^0.0.7",
    "source-map-support": "^0.5.16"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "publishConfig": {
    "access": "public",
    "diretory": "dist"
  }
}
