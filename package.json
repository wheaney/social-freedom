{
  "name": "social-freedom",
  "version": "0.0.1",
  "description": "Open-source social networking framework where each user account owns their infrastructure",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cp -r ./src/user/infrastructure/lambdas/node_modules ./dist/src/user/infrastructure/lambdas",
    "watch": "tsc -w",
    "postinstall": "cd ./src/user/infrastructure/lambdas && npm install",
    "federalstackcheck": "npm run build && cdk -v -a \"ts-node ./src/federal/infrastructure/app.ts\" list",
    "federalstackdeploy": "npm run build && cdk -v -a \"ts-node ./src/federal/infrastructure/app.ts\" deploy",
    "userstackcheck": "npm run build && cdk -v -a \"ts-node ./src/user/infrastructure/app.ts\" list",
    "userstackdeploy": "npm run build && cdk -v -a \"ts-node ./src/user/infrastructure/app.ts\" deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheaney/social-freedom.git"
  },
  "author": "Wayne Heaney",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/wheaney/social-freedom/issues"
  },
  "homepage": "https://github.com/wheaney/social-freedom#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.11.0",
    "@aws-cdk/aws-cloudfront": "^1.11.0",
    "@aws-cdk/aws-cloudtrail": "^1.11.0",
    "@aws-cdk/aws-cloudwatch": "^1.11.0",
    "@aws-cdk/aws-dynamodb": "^1.11.0",
    "@aws-cdk/aws-ec2": "^1.11.0",
    "@aws-cdk/aws-events": "^1.11.0",
    "@aws-cdk/aws-lambda": "^1.11.0",
    "@aws-cdk/aws-route53": "^1.11.0",
    "@aws-cdk/aws-s3": "^1.11.0",
    "@aws-cdk/aws-sns": "^1.11.0",
    "@aws-cdk/core": "^1.11.0",
    "@aws-cdk/cx-api": "^1.11.0",
    "@aws-cdk/region-info": "^1.11.0",
    "@types/aws-lambda": "^8.10.31",
    "@types/jest": "^24.0.18",
    "@types/node": "^8.10.53",
    "@types/uuid": "^3.4.5",
    "aws-cdk": "^1.11.0",
    "aws-sdk-mock": "^4.5.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@aws-cdk/aws-s3-deployment": "^1.11.0",
    "aws-sdk": "^2.525.0",
    "source-map-support": "^0.5.13",
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.11.0",
    "@aws-cdk/aws-lambda": "^1.11.0",
    "@aws-cdk/aws-sns": "^1.11.0",
    "@aws-cdk/core": "^1.11.0",
    "@aws-cdk/cx-api": "^1.11.0",
    "@aws-cdk/region-info": "^1.11.0",
    "source-map-support": "^0.5.9"
  },
  "engines": {
    "node": ">= 8.10.0"
  }
}
